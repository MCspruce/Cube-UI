// headbar
  "headbar": {
    "type": "image",
    "layer": 20,
    "alpha": 0.2,
    "size": [ "100%", 20 ],
    "color": "$global_headbar_color",
    "texture": "textures/ui/white_background",
    "nineslice_size": 1,
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "variables": [
      {
        "requires": "($container_image = '')",
        "$image_ignored": true
      }
    ],
    "$title|default": "$container_title",
    "$image_ignored|default": false,
    "$hide_map|default": true,
    "controls": [
      {
        "container_image": {
          "type": "image",
          "texture": "$container_image",
          "size": [ 16, 16 ],
          "offset": [ 2, 0 ],
          "layer": 21,
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "ignored": "$image_ignored"
        }
      },
      {
        "title": {
          "type": "label",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "offset": [ 20, 0 ],
          "size": [ "default", 10 ],
          "color": "$global_headbar_title_color",
          "layer": 21,
          "text": "$title",
          "font_type": "$global_headbar_title_font",
          "font_scale_factor": 1.1,
          "bindings": [
            {
              "binding_type": "$title_text_binding_type",
              "binding_condition": "$title_binding_condition",
              "binding_name": "$title",
              "binding_name_override": "$title"
            }
          ]
        }
      },
      {
        "right_panel@cube_inventory.headbar_right_panel": {}
      }
    ]
  },

  "headbar_right_panel": {
    "type": "panel",
    "size": "$fill_size",
    "controls": [
      {
        "headbar_right_stack_panel": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "anchor_from": "right_middle",
          "anchor_to": "right_middle",
          "controls": [
            {"destroy_button@headbar_common_right_panel":{"$button_or_toggle":"cube_ui_common.a_destroy_button"}},
            {"destroy_one_button@headbar_common_right_panel":{"$button_or_toggle":"cube_ui_common.a_destroy_one_button"}},
            {"drop_button@headbar_common_right_panel":{"$button_or_toggle":"cube_ui_common.a_drop_button"}},
            {"dropone_button@headbar_common_right_panel":{"$button_or_toggle":"cube_ui_common.a_dropone_button"}},
            {"close_button@cube_inventory.close_button":{}}
          ]
        }
      }
    ]
  },

  "close_button@common.close_button": {
    "anchor_from": "right_middle",
    "anchor_to": "right_middle",
    "offset": [ -2, 0 ],
    "$button_tts_header": "accessibility.button.back"
  },
//////////////////////
// screen
  "main_screen": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%" ],
    "controls": [
      {"headbar@cube_inventory.headbar": {}},
      {"main_half_panel@cube_inventory.main_half_panel": {}}
    ]
  },
  "inventory_screen": {
    "type": "panel",
    "layer": -2,
    "size": "$screen_size",
    "$screen_size|default": [ "85%", "90%" ],
    "controls": [
      {"background@cube_ui_common.container_pocket_background": {}},
      {"main_content@cube_inventory.main_screen": {}},
      {"hold_icon@common.inventory_take_progress_icon_button": {}},
      {"flying_item_renderer@common.flying_item_renderer": {}},
      {"selected_item_details_factory@common.selected_item_details_factory": {}},
      {
        "item_lock_notification_factory@common.item_lock_notification_factory": {
          "$offset": [ 0, "85%" ]
        }
      },
      {
        "game_tip@game_tip.game_tip_panel": {
          "layer": 14,
          "offset": [ 0, "5%" ]
        }
      },
      {"inventory_selected_icon_button@common.inventory_selected_icon_button": {}},
      {"container_gamepad_helpers@common.container_gamepad_helpers": {"layer": 2}},
    ]
  }
