  "headbar_common_right_panel": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%c", "100%" ],
    "controls": [
      {
        "button_or_toggle_panel":{
          "type": "panel",
          "size": [ 16, "100%" ],
