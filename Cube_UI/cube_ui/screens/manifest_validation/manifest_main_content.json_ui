{
  "namespace": "manifest_main_content",
  
  "manifest_validation_scroll_content": {
    "type": "input_panel",
    "offset": [ 0, 20 ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "controls": [
      {
        "scrolling_panel@manifest_main_content.common_scrolling_panel": {
          "layer": 1,
          "size": [ "100% - 4px", "100%" ],
          "offset": [ 2, 0 ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "$scrolling_content": "manifest_main_content.manifest_validation_scroll_panel"
        }
      }
    ]
  },
   
  "common_scrolling_panel@common.scrolling_panel": {
    "layer": 1,
    "$scrolling_pane_size": [ "100% - 1px", "100%" ],
    "$scrolling_content": "$scrolling_content",
    "$scroll_size": [ 5, "100% - 4px" ],
    "$show_background": false,
    "anchor_from": "top_right",
    "anchor_to": "top_right"
  },

  "manifest_validation_scroll_panel@manifest_main_content.common_scroll_panel": {
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "animation_reset_name": "screen_animation_reset",
    "anims": [
      "@cube_ui_anim.headbar_screen_exit_animation_push",
      "@cube_ui_anim.headbar_screen_exit_animation_pop",
      "@cube_ui_anim.headbar_screen_entrance_animation_push",
      "@cube_ui_anim.headbar_screen_entrance_animation_pop"
    ],
    "controls": [
      {
        "pack_error_group@manifest_main_content.pack_error_group": {
          "anchor_from": "top_left",
          "anchor_to": "top_left"
        }
      }
    ]
  },
  
  "common_scroll_panel": {
    "type": "panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ "100%-4px", "100%c" ],
    "offset": [ 2, 0 ]
  },
  
  "pack_error_group": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "top_padding": {
          "type": "panel",
          "size": [ "100%", 10 ]
        }
      },
      {
        "errors@manifest_main_content.pack_error_item": {}
      },
      {
        "padding": {
          "type": "panel",
          "size": [ "100%", 4 ]
        }
      }
    ]
  },
  
  "pack_error_item": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "controls": [
      {
        "secondary@pack_secondary_info.pack_secondary_info": {}
      },
      {
        "error_header": {
          "type": "panel",
          "size": [ "100%", "100%c + 3px" ],
          "controls": [
            {
              "error_text@common.minecraftTenLabel": {
                "size": [ "100%", "default" ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [ 2, 0 ],
                "text": "manifestvalidation.errorlist",
                "color": "$main_header_text_color"
              }
            }
          ]
        }
      },
      {
        "error@error_content.error_content_grid": {}
      },
      {
        "panel": {
          "type": "panel",
          "size": [ 0, 20 ]
        }
      }
    ]
  }
}