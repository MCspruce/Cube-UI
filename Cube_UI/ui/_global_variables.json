/* 中文 / Chinese */
// 这是 Cube UI 全局变量文件。
// 你可以在这修改变量以配置 Cube UI 功能。
// 请不要删除此文件，否则会导致无法进入游戏！
//
// 修改须知：
//  1. 必须检查json格式是否正确！
//  2. 不要删除任何一个变量！
//  3. 不要删除注释 (即 "//")

/* 英文 / English */
// This is the Cube UI global variable file.
// You can modify the variables here to configure the Cube UI function.
// Please do not delete this file, otherwise you will not be able to enter the game!
//
// Modification notice:
//  1. Must check if the json format is correct!
//  2. Don't delete any variables!
//  3. Do not delete comments (eg "//")

  // UI界面 / Global //
     // 全局字体 / 列举 | Global font [List]
     // 默认 - "default"
     // 平滑 - "smooth"
     "$global_main_font": "smooth",

     // 输入框字数限制 / 数字 | Text box word limit [number]
     "$text_box_max_length": 999999,

     // 始终自动放置 / 布尔值 | Always auto place [bool]
     "$always_auto_place": false,

     // 隐藏 HUD 提示 / Hide HUD tip [bool]
     "$hide_hud_tip": true,

     // 启用百分比 / 布尔值 | Enabled percentage [bool]
     "$enabled_percentage": true,

     // 移动物品渲染显示 / 布尔值 | Flying item rendering display [bool]
     "$show_flying_item_renderer": true,

     // 启用容器帮助 / 布尔值 | Enable container help [bool]
     "$enabled_help": true,

     // 增强酿造指南 (需启用容器帮助) / 布尔值 | Enhanced brewing guide (Container Help Required) [bool]
     "$brewing_effect_icon": false,

     // 去除推广 / 布尔值 | remove promotion [bool]
     "$remove_romotion": false,

  // 暂停屏幕 / Pause screen //
     // 启用保存并退出提示 / 布尔值 | Enable quit tip [bool]
     "$enable_quit_tip": true,

  // 关于 UI 屏幕 / About UI screen //
     // 【开发者选项】 无扩展文字 / 颜色 | [Developer] No extended text [color]
      "$no_subpack_text_color": [ 1.0, 0.333, 0.333 ],

  // 设置屏幕 / Setting screen //
    // 隐藏旧世界选项 / 布尔值 | Hide old world options [bool]
    "$old_world_ignored": false,

    // 启用旁观者模式选项 / 布尔值 | enable spectator options [bool]
    "$spectator_enable": true,

  // 容器与交易屏幕 / Containers and Trading screen //
    // 受伤时自动关闭容器屏幕 / 布尔值 | Auto close container screen [bool]
    "$ui_close_on_player_hurt": true,

    // 受伤时自动关闭书与羽毛屏幕 / 布尔值 | Auto close book with feather screen [bool]
    "$book_close_on_player_hurt": true,
 
    // 总是解锁未交易选项 / 布尔值 | Always unlock trade option [bool]
    "$always_unlock_trade_option": false,
 
  // 告示牌编辑屏幕 / Sign screen //
    // 受伤时自动退出告示牌编辑 / 布尔值 | Auto close sign Edit screen [bool]
    "$sign_close_on_player_hurt": true,
 
  // 聊天屏幕 / Chat screen //
    // 受伤时自动关闭聊天屏幕 / 布尔值 | Auto close chat screen [bool]
    "$chat_close_on_player_hurt": false,

    // 启用换行输入 / 布尔值 | Enable newline input [bool]
    "$use_multiline_input": false,

    // 增强命令显示 / 布尔值 | Enhanced command display [bool]
    "$show_more_command": true,

    // 【开发者选项】 是否使用全局字体 / 布尔值 | [Developer] Use global fonts [bool]
    // (启用后，会忽视聊天设置的字体) | (When enabled, the font of chat settings will be ignored)
    "$chat_use_global_main_font": false,

  // 药水效果屏幕 / Effect screen //
    // 受伤时是否自动关闭药水效果屏幕 / 布尔值 | Auto close effect screen [bool]
    "$effect_close_on_player_hurt": true,

  // HUD屏幕 / HUD screen //
    // 物品背景显示 / 布尔值 | Item background display [bool]
    "$show_hud_text_background": true,
    // 【开发者选项】 物品背景 / 路径 | [Developer] Item background [path]
    "$hud_text_background": "textures/ui/hud_tip_text_background",

    // 显示侧边记分板分数 / 布尔值 | Show scoreboard scores [bool]
    "$show_sidebar_score": true,

    // 隐藏Hud时，始终显示准心 / 布尔值 | Always show crosshair [bool]
    "$always_show_cross_hair": false,
    // 隐藏Hud时，始终显示快捷栏 / 布尔值 | Always show shortcut bar [bool]
    "$always_visible_hotbar": false,
    // 启用快捷栏选定改变颜色动画 / 布尔值 | Enable hotbar selection change color animation [bool]
    "$hotbar_selected_anim": false,

    // 【开发者选项】 聊天栏大小 / 大小 | [Developer] Chat bar size [size]
    "$hud_chat_size": [ "100%", "100%c" ],
    // 【开发者选项】 聊天栏最大大小 / 大小 | [Developer] Chat bar max size [size]
    "$hud_chat_max_size": [ "100%", "50%" ],

    // 专注模式和菜单启用 / 布尔值 | Focus mode and menu enable [bool]
    "$focus_on_enable": false,
    // 启用隐藏hud / 布尔值 | Enable hidden hud [bool]
    "$hide_hud_enable": true,
