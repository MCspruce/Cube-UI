//////////////////////////////////
// 这是全局变量(设置UI)文件，你可以在这修改变量
// 
// 修改须知：
//   修改路径(路径以资源包为根目录，文件名不需要带后缀)
//     根目录含文件夹
//     "文件名"
//     文件包含文件夹
//     "文件夹名/文件名"
//   修改布尔值
//     真的(开启) true
//     假的(关闭) false
//   修改大小
//     [ a, b ]
//     a为x轴大小
//     b为y轴大小
//     ab两个分别可以填"百分比","纯数字","百分比 + 纯数字px","百分比 - 纯数字px",
//   修改颜色
//     [ 1, 0.5, 0.999 ]
//     1 - 红色的成分
//     0.5 - 绿色的成分
//     0.999 - 蓝色的成分
//     数字最好不要超过1.0
//   修改数值
//     "$xxx": 9
//     9 - 纯数字，不能添加小数点
//  修改后，千万不要忘记检查json格式！
//
//  自定义UI索引: 
//  https://shimo.im/docs/aBAYVpyy24hdB73j/
////////////////////////////////

  // UI界面 //
     // 背景
       /* 普通背景 */"$default_bg": "cube_ui/textures/background/main",
       /* 黑暗背景 */"$dark_bg": "cube_ui/textures/background/dark_main",
       /* 普通模糊背景 */"$default_blur_bg": "cube_ui/textures/background/blur",
       /* 黑暗模糊背景 */"$dark_blur_bg": "cube_ui/textures/background/dark_blur",

     // 全屏大小
     "$fill_size": [ "100%", "100%" ],
     
     // 全局字体
     "$global_main_font": "smooth",

     // 调试屏幕UI
     "$start_screen_debug": false,

     // 输入框最大字数限制
     "$text_box_max_length": 999999,

     // 快速抢箱
     "$quick_pvp": false,

     // 移动物品渲染显示
     "$show_flying_item_renderer": true,

     // 启用指南/帮助
     "$enabled_help": true,

     // 酿造指南显示药水效果图片(启用指南/帮助时有效)
     "$brewing_effect_icon": false,

  // 开始界面 //
     // 底部文字颜色
     "$start_bottom_text_color": [ 1.0, 1.0, 1.0 ],
     // 底部文字分隔颜色
     "$start_partition_color": [ 0.8, 0.8, 0.8 ],

  // 关于 UI界面 //
     // 文字颜色
       // 没有扩展包文字
        "$no_subpack_text_color": [ 1.0, 0.333, 0.333 ],

  // 设置界面 //
    // 隐藏旧世界选项
    "$old_world_ignored": false,

  // 容器与交易界面 //
    // 玩家受到伤害时是否自动关闭界面
    "$ui_close_on_player_hurt": true,

    // 玩家受到伤害时是否自动关闭书与羽毛界面
    "$book_close_on_player_hurt": true,
 
  // 告示牌编辑界面 //
    // 玩家受到伤害时是否自动关闭界面
    "$sign_close_on_player_hurt": true,
 
  // 聊天界面 //
    // 玩家受到伤害时是否自动关闭界面
    "$chat_close_on_player_hurt": true,

    // 是否使用全局字体
    // (启用后，会忽视聊天设置的字体)
    "$chat_use_global_main_font": false,

  // 药水效果界面 //
    // 玩家受到伤害时是否自动关闭界面
    "$effect_close_on_player_hurt": true,

  // HUD界面 //
    // 物品背景显示
    "$show_hud_text_background": true,
    // 物品背景
    "$hud_text_background": "textures/ui/hud_tip_text_background",

    // 显示侧边记分板分数
    "$show_sidebar_score": true,

    // 隐藏Hud时，始终显示准心
    "$always_show_cross_hair": false,
    // 隐藏Hud时，始终显示快捷栏
    "$always_visible_hotbar": false,
    // 启用快捷栏选定改变颜色动画
    "$hotbar_selected_anim": false,

    // 显示指南针和时钟
      // hide 隐藏全部 | all 显示所有(包括恢复指南针)
      // old 显示所有(不包括恢复指南针)
      // only_clock 仅显示时钟
      // only_compass 仅显示指南针
      "$use_compass_and_clock": "hide",

    // 聊天栏大小
    "$hud_chat_size": [ "100%", "100%c" ],
    // 聊天栏最大大小
    "$hud_chat_max_size": [ "100%", "50%" ],

    // 专注模式和菜单启用
    "$focus_on_enable": true,
