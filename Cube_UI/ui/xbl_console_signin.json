{
  "xbl_console_signin": {
    "button_mappings": [
      {
        "from_button_id": "button.menu_cancel",
        "to_button_id": "button.menu_exit",
        "mapping_type": "global"
      }
    ],
    // 空的顶部面板 - 填充安全角
    "$header_safezone_control": "cube_common.empty_headbar",// 顶部
    "$header_bar_control": "cube_common.empty_headbar",// 侧边
    
    "$screen_bg_content": "cube_background.screen_blur_background_content",
    "$screen_content": "xbl_console_signin.xbl_console_signin_screen_content"
  },

  "xbl_console_signin_screen_content": {
    "controls": [
      { "gamepad_helpers@xbl_console_signin.gamepad_helpers": {} },
      {
        "content@cube_common.headbar_screen": {
          "$title": "xbox.signin",
          "$screen_type": "stack_panel",
          "$main_screen": "xbl_console_signin.main_panel"
        }
      }
    ]
  }
}